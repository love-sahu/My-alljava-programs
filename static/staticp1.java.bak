class staticp1{
	static String s;//global static veriable
	public static void main(String args[]){
		System.out.println(s);//null
		demo();
		String s="love sahu";
		System.out.println(s);//love sahu
		
		System.out.println(staticp1.s);
	}
	public static void demo(){
		s="update name";
		System.out.println(s);
	}
}