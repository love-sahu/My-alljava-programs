class payment extends user
{
	public void cheakPaymentoption(payment payment){
		if(payment instanceof phonepay){
		System.out.println("redirect to phone pay");
		phonepay obj=(phonepay)payment;
		obj.paymentoption();
		}
		else if(payment instanceof Googlepay){
			System.out.println("redirect to Googlepay");
			googlepay obj=(goglepay)payment;
			obj.paymentoption();
		}
		
	}
}




