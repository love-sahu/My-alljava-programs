import java.util.*;
class antiClockwise 
{
	public static void main(String[] args) 
	{
		int arr[]={1,2,3,5,6,2,1,3,6,4};
		System.out.println(Arrays.toString(arr));
		anticlockWise(arr,2);
		System.out.println(Arrays.toString(arr));
	}
	public static void  anticlockWise(int arr[],int times){
		for(int i=0;i<times;i++){
			int temp=arr[arr.length-1];
			for(int j=arr.length-1;j>=0;j--){
				arr[j]=arr[j-1];
			}
				arr[0]=temp;
		}
	}
}
