import java.util.*;
class  assendingBoobleSort
{
	public static void main(String[] args) 
	{
		int arr[]={1,2,3,4,5};
		int cnt=0;
		for(int i=0;i<arr.length;i++){
			for(int j=0;j<arr.length-1-i;j++){
				cnt++;
				if(arr[j]>arr[j+1]){
					int temp=arr[i];
					arr[i]=arr[j+1];
					arr[j+1]=temp;
				}
			}
		}
		System.out.println(Arrays.toString(arr));
		System.out.println(cnt);
	}
}
