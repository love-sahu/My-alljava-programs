class HighestFrequency 
{
	public static void main(String[] args) 
	{
		int arr[]={3,4,5,6,7,8,5,4,4,2,1,1,1,1,1,4,2,2,2,2,2,2,1,1};
		boolean arr2[]=new boolean[arr.length];
		int firstmaxfreqvalue=0;
		int secondmaxfreqvalue=0;
		int secondmax=0;
		int max=Integer.MIN_VALUE;
		for(int i=0;i<arr.length-1;i++){
			int count=0;
			for(int j=0;j<arr.length;j++){
				if((arr[i]==arr[j] )&& arr2[j]==false){
					count++;
					arr2[j]=true;
					}
			}
		
                if (count > max) {
                    secondmax = max;
                    secondmaxfreqvalue = firstmaxfreqvalue;

                    max = count;
                    firstmaxfreqvalue = arr[i];
                } else if (count > secondmax && count < max) {
                    secondmax = count;
                    secondmaxfreqvalue = arr[i];
                }
			}
			System.out.println("firsstHighestFrequency"+firstmaxfreqvalue);
			System.out.println("secondhighestFrequency"+secondmaxfreqvalue);
	}
}
