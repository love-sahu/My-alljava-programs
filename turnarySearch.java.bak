public static int turnarySearch(int arr[],int s,int e,int key){
            int mid1=s+(e-s)/3;
			int mid2=s-(s-s)/3;
			
			if(key==arr[mid1])return mid1;
			 if(key==arr[mid2])return mid2;
			
			if(arr[mid1]>key)  return  turnarySearch( arr[], s, mid1-1, key);
			else if(arr[mid2]<key) return  turnarySearch(arr, mid2+1, mid1-1, key);
			else if(arr[mid1]<key && arr[mid2]>key) {
				
			return	turnarySearch( arr[], mid1+1, mid2-1, key);
				
		}
		return -1;
}




